// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Cliente {
  id         String @id @default(uuid())
  nome       String
  sexo       String
  dataNasc   DateTime
  status     Int 
  created_at DateTime

  @@map("clientes")
}